---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" version="2.0">
    <channel>
        <title>{{ site.title }}</title>
        <link>{{ site.url }}</link>
        <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
        <language>en-us</language>
        <copyright>&#xA9;2016</copyright>
        <webMaster>info@kinlane.com (Kin Lane)</webMaster>
        <managingEditor>info@kinlane.com (Kin Lane)</managingEditor>
        <image>
            <title>{{ site.title| xml_escape }}</title>
            <url>{{ site.url }}{{ site.logo }}</url>
            <link>{{ site.url }}</link>
        </image>
        <itunes:owner>
            <itunes:name>{{ site.author }}</itunes:name>
            <itunes:email>{{ site.author_email }}</itunes:email>
        </itunes:owner>
        <itunes:category text="Technology"></itunes:category>
        <itunes:keywords>{{ site.keywords }}</itunes:keywords>
        <itunes:explicit>no</itunes:explicit>
        <itunes:image href="{{ site.url }}{{ site.logo }}" />
        <itunes:author>{{ site.author | xml_escape }}</itunes:author>
        <itunes:summary>{{ site.summary | xml_escape }}</itunes:summary>
        <itunes:subtitle>{{ site.subtitle | xml_escape }}</itunes:subtitle>
        <description>{{ site.description | xml_escape }}</description>
        {% for podcast in site.data.podcasts limit:25 %}<item>
            <title>{{ podcast.title | xml_escape }}</title>
            <itunes:summary>{{ podcast.summary | xml_escape }}</itunes:summary>
            <itunes:subtitle>{{ podcast.subtitle | xml_escape }}</itunes:subtitle>
            <enclosure url="{{ podcast.audio_file }}" type="audio/mp4" length="{{ podcast.audio_length }}" />
            <guid>{{ podcast.audio_file }}</guid>
            <itunes:duration>{{ podcast.duration }}</itunes:duration>
            <pubDate>{{ podcast.publish_date | date_to_rfc822 }}</pubDate>
        </item>{% endfor %}
    </channel>
</rss>
